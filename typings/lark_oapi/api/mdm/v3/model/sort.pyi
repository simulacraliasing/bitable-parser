from lark_oapi.core.construct import init as init

class Sort:
    field: str | None
    desc: bool | None
    def __init__(self, d=None) -> None: ...
    @staticmethod
    def builder() -> SortBuilder: ...

class SortBuilder:
    def __init__(self) -> None: ...
    def field(self, field: str) -> SortBuilder: ...
    def desc(self, desc: bool) -> SortBuilder: ...
    def build(self) -> Sort: ...
