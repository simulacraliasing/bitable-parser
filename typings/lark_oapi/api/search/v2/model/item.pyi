from .acl import Acl as Acl
from .item_content import ItemContent as ItemContent
from .item_metadata import ItemMetadata as ItemMetadata
from lark_oapi.core.construct import init as init

class Item:
    id: str | None
    acl: list[Acl] | None
    metadata: ItemMetadata | None
    structured_data: str | None
    content: ItemContent | None
    def __init__(self, d=None) -> None: ...
    @staticmethod
    def builder() -> ItemBuilder: ...

class ItemBuilder:
    def __init__(self) -> None: ...
    def id(self, id: str) -> ItemBuilder: ...
    def acl(self, acl: list[Acl]) -> ItemBuilder: ...
    def metadata(self, metadata: ItemMetadata) -> ItemBuilder: ...
    def structured_data(self, structured_data: str) -> ItemBuilder: ...
    def content(self, content: ItemContent) -> ItemBuilder: ...
    def build(self) -> Item: ...
