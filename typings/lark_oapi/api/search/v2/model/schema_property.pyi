class SchemaProperty:
    name: str | None
    type: str | None
    is_searchable: bool | None
    is_sortable: bool | None
    is_returnable: bool | None
    sort_options: SchemaSortOptions | None
    type_definitions: SchemaTypeDefinitions | None
    search_options: SchemaSearchOptions | None
    is_filterable: bool | None
    filter_options: SchemaFilterOptions | None
    answer_option: SchemaFieldAnswerOption | None
    desc: str | None
