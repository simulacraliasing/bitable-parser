from lark_oapi.core.construct import init as init

class Span:
    start: int | None
    end: int | None
    def __init__(self, d=None) -> None: ...
    @staticmethod
    def builder() -> SpanBuilder: ...

class SpanBuilder:
    def __init__(self) -> None: ...
    def start(self, start: int) -> SpanBuilder: ...
    def end(self, end: int) -> SpanBuilder: ...
    def build(self) -> Span: ...
