from .candidate_tag_filter import CandidateTagFilter as CandidateTagFilter
from .degree_filter import DegreeFilter as DegreeFilter
from .range_filter import RangeFilter as RangeFilter
from lark_oapi.core.construct import init as init

class Filter:
    key: str | None
    value_type: int | None
    value_list: list[str] | None
    range_filter: RangeFilter | None
    candidate_tag_filter: CandidateTagFilter | None
    degree_filter: DegreeFilter | None
    def __init__(self, d=None) -> None: ...
    @staticmethod
    def builder() -> FilterBuilder: ...

class FilterBuilder:
    def __init__(self) -> None: ...
    def key(self, key: str) -> FilterBuilder: ...
    def value_type(self, value_type: int) -> FilterBuilder: ...
    def value_list(self, value_list: list[str]) -> FilterBuilder: ...
    def range_filter(self, range_filter: RangeFilter) -> FilterBuilder: ...
    def candidate_tag_filter(self, candidate_tag_filter: CandidateTagFilter) -> FilterBuilder: ...
    def degree_filter(self, degree_filter: DegreeFilter) -> FilterBuilder: ...
    def build(self) -> Filter: ...
