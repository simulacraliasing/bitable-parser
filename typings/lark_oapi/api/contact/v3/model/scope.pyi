from .department import Department as Department
from .user import User as User
from .user_group import UserGroup as UserGroup
from lark_oapi.core.construct import init as init

class Scope:
    departments: list[Department] | None
    users: list[User] | None
    user_groups: list[UserGroup] | None
    def __init__(self, d=None) -> None: ...
    @staticmethod
    def builder() -> ScopeBuilder: ...

class ScopeBuilder:
    def __init__(self) -> None: ...
    def departments(self, departments: list[Department]) -> ScopeBuilder: ...
    def users(self, users: list[User]) -> ScopeBuilder: ...
    def user_groups(self, user_groups: list[UserGroup]) -> ScopeBuilder: ...
    def build(self) -> Scope: ...
